{
  "singleQuote": true,
  "jsxSingleQuote": false,
  "overrides": [
    {
      "files": ["*.scss", "*.css"],
      "options": {
        "singleQuote": false
      }
    },
    {
      "files": ["*-interface.*", "logic-processor*"],
      "options": {
        "printWidth": 120
      }
    }
  ],
  "semi": false,
  "arrowParens": "avoid",
  "trailingComma": "all"
}
