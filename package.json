{
  "name": "cs-gen",
  "version": "0.1.2",
  "description": "Code generator for rapid development using Command Sourcing design pattern.",
  "keywords": [
    "command sourcing",
    "code generation",
    "template",
    "cqrs-exp"
  ],
  "author": {
    "name": "Beeno Tung",
    "email": "aabbcc1241@yahoo.com.hk",
    "url": "https://beeno-tung.surge.sh"
  },
  "license": "BSD-2-Clause",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cs-gen": "cs-gen"
  },
  "scripts": {
    "test": "npm run rawtest",
    "clean": "rimraf node_modules/@beenotung/tslib/index.* dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "postformat": "tslint -p . --fix",
    "prebuild": "npm run clean && tsc-macro",
    "build": "tsc",
    "pretest": "npm run format && npm run build",
    "rawtest": "ts-node test/index.ts",
    "prepublishOnly": "npm run rawtest && npm run build"
  },
  "husky": {
    "hook": {
      "pre-commit": "npm run format"
    }
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/cs-gen.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/cs-gen/issues"
  },
  "homepage": "https://github.com/beenotung/cs-gen#readme",
  "dependencies": {
    "@beenotung/tslib": "^16.34.1",
    "async-mkdirp": "^1.2.7",
    "cli-progress": "^3.6.0",
    "gen-ts-type": "^1.7.1",
    "graceful-fs": "^4.2.3",
    "hasbin": "^1.2.3",
    "mkdirp-sync": "^0.0.3",
    "readline": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.7.1",
    "semver": "^7.3.2",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@nestjs/common": "^7.0.0",
    "@types/cli-progress": "^3.4.1",
    "@types/graceful-fs": "^4.1.3",
    "@types/node": "^8.10.59",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "^7.3.4",
    "husky": "^2.7.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "tsc-macro": "^0.1.3",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-etc": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "optionalDependencies": {
    "@nestjs/common": "^7.0.0"
  }
}
